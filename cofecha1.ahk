;AutoHotkey script for the COFECHA program by Michael O. Schwartz 2024
; Run the COFECHA application
Run, "C:\cofecha\cofecha.exe" 
Sleep, 5000 
    WinActivate "C:\cofecha\cofecha.exe" ; Ensure the window is open

    ; Send keystrokes with delays
    Send, br050  ; 5-character identifier for the tree-ring file brit050.rwl
    Sleep, 2000 ; Adjust the delay as necessary
    Send, {Enter}
    Sleep, 2000
    Send, C:\cofecha\brit050.rwl ; replace with suitable tree-ring series file name *.rwl
    Sleep,2000
    Send, {Enter}
    Sleep,2000
    Send, {Enter}
    Sleep,2000
    Send, C:\cofecha\germ064.rwl ; use "search/replace" for a different undated single-series file or chronology
    Sleep,2000
    Send, {Enter}
    Sleep,2000
    Send, {Enter}
    Sleep,2000
    Send, ringe
    Sleep,2000
    Send, {Enter}
    Sleep,2000
    Send, 8
    Sleep,2000
    Send, {Enter}
    Sleep,2000
    Send, 8
    Sleep,4000
    Send, {Enter}
    Sleep,2000
    Send, {Enter}
    Sleep,2000
    Send, {Enter}
    Sleep,2000
    Send, {Enter}

; Save the output
    Send, ^s ; Ctrl+S to save the document
    Sleep, 2000
    Send, out6.rwl ; Name the output file
    Sleep, 2000
    Send, {Enter} ; Confirm the save
    Sleep, 2000

    ; Close the COFECHA application
    Send, !{F4} ; Alt+F4 to close the window

   

